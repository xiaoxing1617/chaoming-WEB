xt="{{msg.confirm_txt}}" title="提示" content="{{msg.content}}" data-event-opts="{{[['^confirm',[['e0']]],['^close',[['e1']]]]}}" bind:confirm="__e" bind:close="__e" bind:__l="__l"></uni-popup-dialog></uni-popup><uni-popup class="vue-ref" vue-id="43b86140-3" data-ref="brushPopup" bind:__l="__l" vue-slots="{{['default']}}"><view class="brush-popup-view"><view class="main"><view class="title">{{testAnswer.title}}</view><view class="answer-list"><block wx:for="{{testAnswer.list}}" wx:for-item="item" wx:for-index="index"><view class="item"><view class="topic"><label class="_span">{{index+1+'.'}}</label>{{''+item.title}}</view><view class="answer">{{item.op}}</view></view></block></view></view></view></uni-popup></view>